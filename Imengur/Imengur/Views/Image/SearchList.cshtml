@model IList<Imengur.Models.Image>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<h1>Uploaded images</h1>
<table class="table">
    <tr>
        <th>Image ID</th>
        <th>Title</th>
        @*<th>Date</th>*@
        <th>Image</th>
        <th>Description</th>
    </tr>
    <tbody>
        @foreach (var image in @Model)
        {
            <tr>
                <td>
                    @image.id
                </td>
                <td>
                    @image.Title
                </td>
                @*<td>
                        @image.UploadDate
                    </td>*@
                <td>
                    @image.ImageData
                </td>
                <td>
                    @image.Description
                </td>
            </tr>
        }
    </tbody>
</table>
