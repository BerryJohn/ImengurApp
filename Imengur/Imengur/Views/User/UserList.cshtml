@model ICollection<Imengur.Models.User>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<h1>Lista aktualnych użytkowników:</h1>
<table class="table">
    <tr>
        <th>Imie</th>
        <th>Login</th>
        <th>Email</th>
    </tr>
    <tbody>
        @foreach(var user in @Model)
            { 
        <tr>
            <td>
                @user.Name
            </td>
            <td>
                @user.Login
            </td>
            <td>
                @user.Email
            </td>
        </tr>
            }
    </tbody>
</table>
